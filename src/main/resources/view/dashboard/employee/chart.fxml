<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="394.0" prefWidth="896.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.pos.controller.dashboard.employee.ChartController">
   <children>
      <BarChart fx:id="chart" animated="false" prefHeight="394.0" prefWidth="896.0" title="Employee Report">
         <xAxis>
            <CategoryAxis animated="false" label="Months" prefHeight="60.0" prefWidth="766.0" side="BOTTOM" tickLabelFill="#2f3548">
               <tickLabelFont>
                  <Font size="10.0" />
               </tickLabelFont>
            </CategoryAxis>
         </xAxis>
         <yAxis>
            <NumberAxis animated="false" label="Employee" minorTickCount="0" prefHeight="238.0" prefWidth="60.0" side="LEFT" tickLabelFill="#2f3548" upperBound="10000.0">
               <tickLabelFont>
                  <Font size="10.0" />
               </tickLabelFont>
            </NumberAxis>
         </yAxis>
      </BarChart>
   </children>
</VBox>
